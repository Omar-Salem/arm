<?xml version="1.0" ?>
<robot name="arm" xmlns:xacro="http://www.ros.org/wiki/xacro">

    <xacro:include filename="$(find arm_description)/urdf/materials.xacro"/>
    <xacro:property name="PI" value="3.14159265359"/>
    <xacro:property name="effort" value="30"/>
    <xacro:property name="velocity" value="10"/>

    <link name="base_link">
        <inertial>
            <origin xyz="0.0 0.0 0.057499999999999975" rpy="0 0 0"/>
            <mass value="5.122656437250909"/>
            <inertia ixx="0.007959" iyy="0.007959" izz="0.004626" ixy="-0.0" iyz="0.0" ixz="0.0"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find arm_description)/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find arm_description)/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <link name="shoulder_1">
        <inertial>
            <origin xyz="6.8492023198216155e-18 -2.816067379285414e-11 0.0467352918800006" rpy="0 0 0"/>
            <mass value="5.211945183934523"/>
            <inertia ixx="0.008209" iyy="0.004805" izz="0.008038" ixy="-0.0" iyz="-0.0" ixz="-0.0"/>
        </inertial>
        <visual>
            <origin xyz="-0.0 -0.0 -0.115" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find arm_description)/meshes/shoulder_1.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <origin xyz="-0.0 -0.0 -0.115" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find arm_description)/meshes/shoulder_1.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <link name="should_biceptricep_link_1">
        <inertial>
            <origin xyz="-0.06680915249598346 -0.009999999999999933 -8.326672684688674e-16" rpy="0 0 0"/>
            <mass value="2.4998312500297173"/>
            <inertia ixx="0.001444" iyy="0.009664" izz="0.008376" ixy="0.0" iyz="0.0" ixz="-0.0"/>
        </inertial>
        <visual>
            <origin xyz="-0.0 0.0575 -0.1625" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find arm_description)/meshes/should_biceptricep_link_1.stl"
                      scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <origin xyz="-0.0 0.0575 -0.1625" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find arm_description)/meshes/should_biceptricep_link_1.stl"
                      scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <link name="biceptricep_1">
        <inertial>
            <origin xyz="1.1102230246251565e-16 0.057500000000000065 -8.326672684688674e-17" rpy="0 0 0"/>
            <mass value="5.122656437250909"/>
            <inertia ixx="0.007959" iyy="0.004626" izz="0.007959" ixy="-0.0" iyz="0.0" ixz="0.0"/>
        </inertial>
        <visual>
            <origin xyz="0.2 0.0775 -0.1625" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find arm_description)/meshes/biceptricep_1.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <origin xyz="0.2 0.0775 -0.1625" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find arm_description)/meshes/biceptricep_1.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <link name="biceptricep_forearm_link_1">
        <inertial>
            <origin xyz="-0.06680915249598346 0.010000000000000002 6.661338147750939e-16" rpy="0 0 0"/>
            <mass value="2.4998312500297173"/>
            <inertia ixx="0.001444" iyy="0.009664" izz="0.008376" ixy="0.0" iyz="-0.0" ixz="0.0"/>
        </inertial>
        <visual>
            <origin xyz="0.2 -0.0375 -0.1625" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find arm_description)/meshes/biceptricep_forearm_link_1.stl"
                      scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <origin xyz="0.2 -0.0375 -0.1625" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find arm_description)/meshes/biceptricep_forearm_link_1.stl"
                      scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <link name="forearm_1">
        <inertial>
            <origin xyz="0.0 0.0575 0.0" rpy="0 0 0"/>
            <mass value="5.122656437250909"/>
            <inertia ixx="0.007959" iyy="0.004626" izz="0.007959" ixy="-0.0" iyz="0.0" ixz="0.0"/>
        </inertial>
        <visual>
            <origin xyz="0.4 -0.0375 -0.1625" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find arm_description)/meshes/forearm_1.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <origin xyz="0.4 -0.0375 -0.1625" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find arm_description)/meshes/forearm_1.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <link name="wrist_1">
        <inertial>
            <origin xyz="-1.1102230246251565e-16 0.04673529188000053 2.816077926404148e-11" rpy="0 0 0"/>
            <mass value="5.211945183934523"/>
            <inertia ixx="0.008209" iyy="0.008038" izz="0.004805" ixy="0.0" iyz="0.0" ixz="0.0"/>
        </inertial>
        <visual>
            <origin xyz="0.4 -0.1525 -0.1625" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find arm_description)/meshes/wrist_1.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <origin xyz="0.4 -0.1525 -0.1625" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find arm_description)/meshes/wrist_1.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <joint name="waist_joint" type="revolute">
        <origin xyz="0.0 0.0 0.115" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="shoulder_1"/>
        <axis xyz="0.0 0.0 1.0"/>
        <limit lower="-${PI/2}" upper="${PI/2}" effort="${effort}" velocity="${velocity}"/>
    </joint>

    <joint name="shoulder_joint" type="revolute">
        <origin xyz="0.0 -0.0575 0.0475" rpy="0 0 0"/>
        <parent link="shoulder_1"/>
        <child link="should_biceptricep_link_1"/>
        <axis xyz="0.0 -1.0 -0.0"/>
        <limit lower="-${PI/2}" upper="${PI/2}" effort="${effort}" velocity="${velocity}"/>
    </joint>

    <joint name="Rigid 7" type="fixed">
        <origin xyz="-0.2 -0.02 0.0" rpy="0 0 0"/>
        <parent link="should_biceptricep_link_1"/>
        <child link="biceptricep_1"/>
    </joint>

    <joint name="biceptricep_joint" type="revolute">
        <origin xyz="0.0 0.115 0.0" rpy="0 0 0"/>
        <parent link="biceptricep_1"/>
        <child link="biceptricep_forearm_link_1"/>
        <axis xyz="0.0 1.0 0.0"/>
        <limit lower="-${PI/2}" upper="${PI/2}" effort="${effort}" velocity="${velocity}"/>
    </joint>

    <joint name="Rigid 8" type="fixed">
        <origin xyz="-0.2 0.0 0.0" rpy="0 0 0"/>
        <parent link="biceptricep_forearm_link_1"/>
        <child link="forearm_1"/>
    </joint>

    <joint name="forearm_joint" type="revolute">
        <origin xyz="0.0 0.115 0.0" rpy="0 0 0"/>
        <parent link="forearm_1"/>
        <child link="wrist_1"/>
        <axis xyz="-0.0 -1.0 0.0"/>
        <limit lower="-${PI/2}" upper="${PI/2}" effort="${effort}" velocity="${velocity}"/>
    </joint>

    <link name="world"/>

    <joint name="virtual_joint" type="fixed">
        <parent link="world"/>
        <child link="base_link"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>


</robot>
