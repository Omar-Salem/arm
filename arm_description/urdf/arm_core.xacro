    <link name="bb_1_1">
        <inertial>
            <origin xyz="-0.00019632988035193946 0.006345060504398406 0.09901048510996069" rpy="0 0 0"/>
            <mass value="3.341835621962638"/>
            <inertia ixx="0.009045" iyy="0.009197" izz="0.002746" ixy="-0.0" iyz="0.000632" ixz="8e-06"/>
