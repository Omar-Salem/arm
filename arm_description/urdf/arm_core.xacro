    <joint name="waist_joint" type="revolute">
        <origin xyz="0.0 0.0 0.1" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="aa_1_1"/>
        <axis xyz="0.0 0.0 1.0"/>
        <limit lower="-${PI}" upper="${PI}" effort="${effort}" velocity="${velocity}"/>
    </joint>

    <joint name="shoulder_joint" type="revolute">
        <origin xyz="1.8e-05 -0.003 0.060821" rpy="0 0 0"/>
        <parent link="aa_1_1"/>
        <child link="bb_1_1"/>
        <axis xyz="0.0 1.0 0.0"/>
        <limit lower="-1.833" upper="1.833" effort="${effort}" velocity="${velocity}"/>
    </joint>

    <joint name="biceptricep_joint" type="revolute">
        <origin xyz="-0.000222 0.003204 0.152179" rpy="0 0 0"/>
        <parent link="bb_1_1"/>
        <child link="aa_2_1"/>
        <axis xyz="0.0 -0.0 1.0"/>
        <limit lower="-${PI}" upper="${PI}" effort="${effort}" velocity="${velocity}"/>
    </joint>

    <joint name="forearm_joint" type="revolute">
        <origin xyz="1.8e-05 -0.003 0.060821" rpy="0 0 0"/>
        <parent link="aa_2_1"/>
        <child link="bb_2_1"/>
        <axis xyz="0.0 1.0 0.0"/>
        <limit lower="-1.833" upper="1.833" effort="${effort}" velocity="${velocity}"/>
    </joint>

    <joint name="wrist_joint" type="revolute">
        <origin xyz="-0.000222 0.003204 0.152179" rpy="0 0 0"/>
        <parent link="bb_2_1"/>
        <child link="Actuator_Cover_v4_1"/>
        <axis xyz="0.0 -0.0 1.0"/>
        <limit lower="-${PI}" upper="${PI}" effort="${effort}" velocity="${velocity}"/>
    </joint>

    <joint name="hand_joint" type="fixed">
        <origin xyz="0.0 0.0 0.0105" rpy="0 0 0"/>
        <parent link="Actuator_Cover_v4_1"/>
        <child link="hand_1"/>
    </joint>

    <joint name="finger_joint" type="prismatic">
        <origin xyz="0.0 -0.045 0.005" rpy="0 0 0"/>
        <parent link="hand_1"/>
        <child link="finger_1"/>
        <axis xyz="0.0 -1.0 -0.0"/>
        <limit lower="-0.09" upper="0.0" effort="100" velocity="100"/>
    </joint>

</robot>
