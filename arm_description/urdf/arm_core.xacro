    <joint name="finger_joint" type="prismatic">
        <origin xyz="0.044778 0.003204 0.102179" rpy="0 0 0"/>
        <parent link="B_link_v3_1"/>
        <child link="finger_1"/>
        <axis xyz="1.0 -0.0 0.0"/>
