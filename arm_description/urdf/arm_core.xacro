    <joint name="waist_joint" type="revolute">
        <origin xyz="0.0 0.0 0.1" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="A_link_1_1"/>
        <axis xyz="0.0 0.0 1.0"/>
        <limit lower="-${PI}" upper="${PI}" effort="${effort}" velocity="${velocity}"/>
    </joint>

    <joint name="Rigid 2" type="fixed">
        <origin xyz="1.8e-05 -0.058 0.060821" rpy="0 0 0"/>
        <parent link="A_link_1_1"/>
        <child link="mini_1"/>
    </joint>

    <joint name="shoulder_joint" type="revolute">
        <origin xyz="0.0 0.055 0.0" rpy="0 0 0"/>
        <parent link="mini_1"/>
        <child link="B_link_1_1"/>
        <axis xyz="-0.0 -1.0 0.0"/>
        <limit lower="-1.833" upper="1.833" effort="${effort}" velocity="${velocity}"/>
    </joint>

    <joint name="Rigid 4" type="fixed">
        <origin xyz="-0.000222 0.003204 0.097179" rpy="0 0 0"/>
        <parent link="B_link_1_1"/>
        <child link="mini_2_1"/>
